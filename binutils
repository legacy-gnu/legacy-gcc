GNUCC = gcc -O
CFLAGS = -g

all: gprof ld size nm strip make

ld: ld.o
	$(CC) -o ld ld.o

size: size.o
	$(CC) -o size size.o

nm: nm.o
	$(CC) -o nm nm.o

strip: strip.o
	$(CC) -o strip strip.o

gprof: gprof.o
	$(CC) -o gprof gprof.o
gprof.o: gprof.c
	$(GNUCC) -c $(CFLAGS) gprof.c

make: make.o glob.o arscan.o
	$(CC) -o make make.o glob.o arscan.o
